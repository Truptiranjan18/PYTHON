#Gussing Game
secret_number=30
guess_count=0
guess_limit=3
while(guess_count<guess_limit):
    guess=int(input("Guess Your Lucky Number(Between 28 to 36) : "))
    guess_count +=1
    if(guess==secret_number):
        print("you won!")
        break
else:
    print("sorry! you failled")
